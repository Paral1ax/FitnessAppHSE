<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.mir.fitnessapplication">

<uses-permission android:name="android.permission.INTERNET" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.FitnessApplication">
    <activity
        android:name=".entry.ui.register.username.UsernameInputActivity"
        android:theme="@android:style/Theme.NoTitleBar">
        <intent-filter>

        </intent-filter>
    </activity>
    <activity
        android:name=".entry.ui.register.data.DataActivity"
        android:theme="@android:style/Theme.NoTitleBar"/>
    <activity
        android:name=".entry.ui.register.code.EnterCodeActivity"
        android:theme="@android:style/Theme.NoTitleBar"/>
    <activity
        android:name=".entry.ui.register.RegisterActivity"
        android:theme="@android:style/Theme.NoTitleBar"/>
    <activity
        android:name=".entry.ui.login.LoginActivity"
        android:theme="@android:style/Theme.NoTitleBar"/>
    <activity
        android:name=".entry.ui.entry.EntryActivity"
        android:theme="@android:style/Theme.NoTitleBar">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name="com.mir.fitnessapplication.main.MainActivity"
        android:label="@string/title_activity_main"
        android:theme="@android:style/Theme.NoTitleBar">
    </activity>
</application>

</manifest>

